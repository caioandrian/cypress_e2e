    "cypress-axe": "^1.5.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-delete-downloads-folder": "0.0.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.8.0",
    "faker-br": "^0.4.1",
    "fs-extra": "^11.1.1",
