{
  "name": "cypress_e2e",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open-hmg": "cypress open --env fileConfig=hmg",
    "cy:open-pre": "cypress open --env fileConfig=pre",

    "cy:run-exemplo": "cypress run --spec 'cypress/e2e/spec/exemplo/' --browser chrome --headless",
    "cy:run-exemplo-regressao": "cypress run --spec 'cypress/e2e/spec/exemplo/' --browser chrome --headless --env grep=regress√£o",

    "cy:run-all-chrome": "cypress run --browser chrome",
    "cy:run-all-edge": "cypress run --browser edge",
    "cy:run-all-firefox": "cypress run --browser firefox",
    "cy:run-all-electron": "cypress run --browser electron",
    
    "cypress:verify": "cypress verify",
    "update-packages": "npx npm-check-updates"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "./cypress/e2e/step_definitions",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/json"
    }
  },
  "keywords": [],
